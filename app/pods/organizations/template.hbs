{{#item-list
  items=model.localOrganizations
  on-item-select=(route-action "itemSelected")
  as |organization|
}}
  <img src={{organization.iconUrl}}>
  {{organization.name}}
  {{fa-icon
    (favorite-icon-for model.favorites.items organization)
    click=(route-action "favoriteOrganization" organization)
  }}
{{else}}
  No organizations.
{{/item-list}}

